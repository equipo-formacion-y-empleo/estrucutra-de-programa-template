{
  "exercises": [
    "Exercise01_HelloWorld",
    "Exercise02_VariablesScope", 
    "Exercise04_TypeConversions",
    "Exercise05a_TriangleArea",
    "Exercise05b_TemperatureConverter",
    "Exercise05c_Average",
    "Exercise05d_Hypotenuse",
    "Exercise05e_BMI",
    "Exercise05f_Distance",
    "Exercise11_RestaurantBill",
    "Exercise12_CurrencyConverter",
    "Exercise13_GeometricShapes"
  ],
  "testCases": {
    "Exercise01_HelloWorld": [
      {
        "arguments": ["María", "Carlos"],
        "expectedOutputs": ["Hello, María!", "Hello, Carlos!"]
      },
      {
        "arguments": ["Ana"],
        "expectedOutputs": ["Hello, Ana!", "Hello, Nombre2!"]
      }
    ],
    "Exercise02_VariablesScope": [
      {
        "arguments": ["4,0", "6,0", "5,0", "7,0"],
        "expectedOutputs": ["Suma total de superficies: 59"]
      },
      {
        "arguments": ["2,5", "3,5", "4,5", "5,5"],
        "expectedOutputs": ["Suma total de superficies: 33,5"]
      },
      {
        "arguments": ["1,0", "1,0", "2,0", "2,0"],
        "expectedOutputs": ["Suma total de superficies: 5"]
      }
    ],   
    "Exercise04_TypeConversions": [
      {
        "arguments": ["23,89", "17"],
        "expectedOutputs": ["23,89", "23", "17,0"]
      },
      {
        "arguments": ["45,67", "25"],
        "expectedOutputs": ["45,67", "45", "25,0"]
      },
      {
        "arguments": ["100,99", "50"],
        "expectedOutputs": ["100,99", "100", "50,0"]
      }
    ],
    "Exercise05a_TriangleArea": [
      {
        "arguments": ["8,5", "3,2"],
        "expectedOutputs": ["Área del triángulo: 13,60"]
      },
      {
        "arguments": ["10,0", "5,0"],
        "expectedOutputs": ["Área del triángulo: 25,00"]
      },
      {
        "arguments": ["6,0", "4,0"],
        "expectedOutputs": ["Área del triángulo: 12,00"]
      }
    ],
    "Exercise05b_TemperatureConverter": [
      {
        "arguments": ["0"],
        "expectedOutputs": ["0,00°C", "32,00°F"]
      },
      {
        "arguments": ["100"],
        "expectedOutputs": ["100,00°C", "212,00°F"]
      },
      {
        "arguments": ["25"],
        "expectedOutputs": ["25,00°C", "77,00°F"]
      }
    ],
    "Exercise05c_Average": [
      {
        "arguments": ["10", "20", "30"],
        "expectedOutputs": ["Average: 20"]
      },
      {
        "arguments": ["5", "15", "25"],
        "expectedOutputs": ["Average: 15"]
      },
      {
        "arguments": ["1", "2", "3"],
        "expectedOutputs": ["Average: 2"]
      }
    ],
    "Exercise05d_Hypotenuse": [
      {
        "arguments": ["3", "4"],
        "expectedOutputs": ["Hypotenuse: 5"]
      },
      {
        "arguments": ["5", "12"],
        "expectedOutputs": ["Hypotenuse: 13"]
      },
      {
        "arguments": ["8", "6"],
        "expectedOutputs": ["Hypotenuse: 10"]
      }
    ],
    "Exercise05e_BMI": [
      {
        "arguments": ["70", "1,75"],
        "expectedOutputs": ["BMI: 22,86"]
      },
      {
        "arguments": ["80", "1,80"],
        "expectedOutputs": ["BMI: 24,69"]
      },
      {
        "arguments": ["65", "1,70"],
        "expectedOutputs": ["BMI: 22,49"]
      }
    ],
    "Exercise05f_Distance": [
      {
        "arguments": ["0", "0", "3", "4"],
        "expectedOutputs": ["Distance: 5"]
      },
      {
        "arguments": ["1", "1", "4", "5"],
        "expectedOutputs": ["Distance: 5"]
      },
      {
        "arguments": ["0", "0", "6", "8"],
        "expectedOutputs": ["Distance: 10"]
      }
    ],
    "Exercise11_RestaurantBill": [
      {
        "arguments": ["100,0", "0,21", "0,15", "4"],
        "expectedOutputs": ["Total por persona: €34,00"]
      },
      {
        "arguments": ["80,0", "0,18", "0,20", "2"],
        "expectedOutputs": ["Total por persona: €55,20"]
      },
      {
        "arguments": ["150,0", "0,21", "0,10", "6"],
        "expectedOutputs": ["Total por persona: €32,75"]
      }
    ],
    "Exercise12_CurrencyConverter": [
      {
        "arguments": ["50,0"],
        "expectedOutputs": ["€50,00 = $54,50 USD", "€50,00 = ¥8175,00 JPY", "€50,00 = £43,00 GBP"]
      },
      {
        "arguments": ["200,0"],
        "expectedOutputs": ["€200,00 = $218,00 USD", "€200,00 = ¥32700,00 JPY", "€200,00 = £172,00 GBP"]
      },
      {
        "arguments": ["75,0"],
        "expectedOutputs": ["€75,00 = $81,75 USD", "€75,00 = ¥12262,50 JPY", "€75,00 = £64,50 GBP"]
      }
    ],
    "Exercise13_GeometricShapes": [
      {
        "arguments": ["10,0"],
        "expectedOutputs": ["Triángulo Equilátero: área = 43,30", "Cuadrado: área = 100,00", "Hexágono Regular: área = 259,81"]
      },
      {
        "arguments": ["5,0"],
        "expectedOutputs": ["Triángulo Equilátero: área = 10,83", "Cuadrado: área = 25,00", "Hexágono Regular: área = 64,95"]
      },
      {
        "arguments": ["15,0"],
        "expectedOutputs": ["Triángulo Equilátero: área = 97,43", "Cuadrado: área = 225,00", "Hexágono Regular: área = 584,57"]
      }
    ]
  }
}