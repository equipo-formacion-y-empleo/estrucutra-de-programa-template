  "testCases": {
    "Exercise01_HelloWorld": [
      {
        "arguments": ["María", "Carlos"],
        "expectedOutputs": ["Hello, María!", "Hello, Carlos!"]
      },
      {
        "arguments": ["Ana"],
        "expectedOutputs": ["Hello, Ana!", "Hello, Nombre2!"]
      }
    ],
    "Exercise02_VariablesScope": [
      {
        "arguments": ["4,0", "6,0", "5,0", "7,0"],
        "expectedOutputs": ["Suma total de superficies: 59"]
      },
      {
        "arguments": ["2,5", "3,5", "4,5", "5,5"],
        "expectedOutputs": ["Suma total de superficies: 33,5"]
      },
      {
        "arguments": ["1,0", "1,0", "2,0", "2,0"],
        "expectedOutputs": ["Suma total de superficies: 5"]
      }
    ],   
    "Exercise04_TypeConversions": [
      {
        "arguments": ["23,89", "17"],
        "expectedOutputs": ["23,89", "23", "17,0"]
      },
      {
        "arguments": ["45,67", "25"],
        "expectedOutputs": ["45,67", "45", "25,0"]
      },
